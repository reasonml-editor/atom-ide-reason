{
  "name": "ide-reason",
  "main": "./lib/main",
  "private": true,
  "version": "0.1.8",
  "description": "ReasonML/OCaml language support for Atom-IDE.",
  "author": {
    "name": "zaaack",
    "email": "zaaack@qq.com",
    "url": "https://github.com/zaaack"
  },
  "homepage": "https://github.com/reasonml-editor/atom-ide-reason",
  "keywords": [
    "ide",
    "reason",
    "reasonml",
    "ocaml",
    "bucklescript",
    "language-server-protocol"
  ],
  "activationCommands": {},
  "repository": "https://github.com/reasonml-editor/atom-ide-reason",
  "bugs": {
    "url": "https://github.com/reasonml-editor/atom-ide-reason/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-languageclient": "^0.7.3",
    "atom-package-deps": "^4.6.1",
    "ocaml-language-server": "1.0.13"
  },
  "package-deps": [
    "atom-ide-ui:0.7.0",
    "language-reason:0.0.4",
    "language-ocaml:1.9.5"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "nuclide-datatip.provider": {
      "versions": {
        "0.0.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "nuclide-code-format.provider": {
      "versions": {
        "0.0.0": "provideCodeFormat"
      }
    },
    "nuclide-definition-provider": {
      "versions": {
        "0.0.0": "provideDefinitions"
      }
    },
    "nuclide-find-references.provider": {
      "versions": {
        "0.0.0": "provideFindReferences"
      }
    },
    "nuclide-outline-view": {
      "versions": {
        "0.0.0": "provideOutlines"
      }
    }
  }
}
